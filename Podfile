# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

source 'https://github.com/CocoaPods/Specs.git'

target 'PGH Bus Tracker' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for PGH Bus Tracker
  pod 'GoogleMaps'
  pod 'Alamofire', '~> 4.4'
  pod 'SWXMLHash', '~> 3.0.0'

  target 'PGH Bus TrackerTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PGH Bus TrackerUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
